meta {
  name: trips without details
  type: http
  seq: 8
}

get {
  url: {{host}}/api/trips/list?deviceId={{deviceId}}
  body: none
  auth: inherit
}

params:query {
  deviceId: {{deviceId}}
  ~page: 1
  ~pageSize: 1
}

vars:post-response {
  tripId: res.body[0].id
}

settings {
  encodeUrl: true
  timeout: 0
}
